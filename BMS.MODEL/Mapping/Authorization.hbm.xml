<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="BMS.MODEL"
                   namespace="BMS.MODEL.Entity">

	<class name="Authorization" table="BMS.Authorizations">
		<id name="Id_Authorization" column="Id_Authorization" type ="int">
			<generator class="identity"/>
		</id>
		<property name="StartDate" column="StartDate" type="datetime"/>
		<property name="EndDate" column="EndDate" type="datetime"/>
		<property name="Id_User" column="Id_User" type="int"/>
	</class>

</hibernate-mapping>