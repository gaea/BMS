<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="TMA.MODEL"
                   namespace="TMA.MODEL.Entity">

	<class name="City" table="dbo.Tbl_Ciudades">
		<id name="Id_City" column="Ciudad" type="int">
			<generator class="increment"/>
		</id>
		<property name="Name" column="NombreCiudad" type="string" />
		<property name="Id_Department" column="Departamento" type="int" />
		<property name="DaneCode" column="CodigoDane" type="int" />
	</class>

</hibernate-mapping>